import { describe, test, expect, Result } from './tests.uts'

export function testSet(): Result {
    return describe("Set", () => {
        test('size', () => {
            const set1 = new Set<Any>();
            const object1 = {};
            set1.add(42);
            set1.add('forty two');
            set1.add('forty two');
            set1.add(object1);
            expect(set1.size).toEqual(3);
        })
