        })
        test('has', () => {
            const set1 = new Set<number>([1, 2, 3, 4, 5]);
            expect(set1.has(1)).toEqual(true);
            expect(set1.has(5)).toEqual(true);
            expect(set1.has(6)).toEqual(false);
        })
